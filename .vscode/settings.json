{
    "search.exclude": {
        ".git": true,
        "lib": true,
        "node_modules": true,
        "npm-debug.log.*": true,
        "test/**/__snapshots__": true,
        "package-lock.json": true,
        "yarn.lock": true,
        "**/__snapshots__": true,
        "README.md": true,
    },
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit"
    }
}